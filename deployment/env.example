DATABASE_URL=your_database_url_here
MONGODB_URI=mongodb://localhost:27017/coconut_leaf_detection

# Twilio (optional; guards will silence logs if not set)
TWILIO_ACCOUNT_SID=your_twilio_account_sid_here # must start with AC...
TWILIO_AUTH_TOKEN=your_twilio_auth_token_here
TWILIO_PHONE_NUMBER=your_twilio_phone_number_here

# Cloudinary (required for profile images)
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Profile image size limits
MAX_PROFILE_IMAGE_MB=5

FLASK_API_URL=http://localhost:5000/api
SECRET_KEY=your_secret_key_here
NODE_ENV=development
PORT=3000